{
    "manifest_version": 2,
    "name": "GitHub",
    "version": "1.0",
    "description": "An extension for injecting custom CSS/JS to the GitHub website",
    "author": "Pramod Ghuge",
    "page_action": {
          "default_icon": "icon.png",
          "default_title": "GitHub" 
        },
    "content_scripts": [{
        "matches": ["https://github.com/*"],
        "css": ["myStyles.css"],
        "js": ["contentScript.js"]
    }]
}